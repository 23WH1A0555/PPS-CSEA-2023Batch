#include<stdio.h>
int main(){
int month;
printf("enter month number");
scanf("%d",&month);
switch(month){
case 1:
	printf("no. of days:30");
	break;
case 2:
	printf("no. of days:29");
	break;
case 3:
	printf("no. of days:30");
	break;
case 4:
	printf("no. of days:31");
	break;
case 5:
	printf("no. of days:30");
	break;
case 6:
	printf("31");
	break;
case 7:
	printf("30");
	break;
case 8:
	printf("31");
	break;
case 9:
	printf("30");
	break;
case 10:
	printf("31");
	break;
case 11:
	printf("30");
	break;
case 12:
	printf("31");
	break;
default:
	printf("entered month is not defined");
}
return 0;
}
