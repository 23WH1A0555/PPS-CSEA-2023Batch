/*This program is to find sum of even numbers in fibonocci series */
#include <stdio.h>

int main() {
              int n1 = 0,n2 = 1,n3,sum =0;
              for(int i = 1 ; i < 20 ; i++){
                  n3 = n1 + n2;
                  if(n3%2==0){
                     sum = sum + n3;
                     
                  }
                 n1 = n2;
                 n2 = n3;
              }
              
        printf("%d",sum);
    return 0;
}
