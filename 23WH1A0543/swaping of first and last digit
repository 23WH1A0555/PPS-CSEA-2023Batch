//swaping of  first and last digit of a number
#include<stdio.h>
int main()
{
  int a[10],temp,i,n;
  printf("Enter range of array");
  scanf("%d",&n);
  for(i=0;i<n;i++){
    scanf("%d",&a[i]);
  }
  temp=a[n-1];
  a[n-1]=a[0];
  a[0]=temp;
  for(i=0;i<n;i++){
    printf("%d",a[i]);
  }
  return 0;
}
