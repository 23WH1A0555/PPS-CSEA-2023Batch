#include<stdio.h>
int main(){
int arr1[10],arr2[10];
printf("enter 10 elements :");
for(int i=0;i<10;i++){
	scanf("%d",&arr1[i]);
}
printf("array1:");
for(int i=0;i<10;i++){

	printf("%d ",arr1[i]);
}
for(int i=0;i<10;i++){
	arr2[i]=arr1[i];
}
printf("\n");
printf("array2:");
for(int i=0;i<10;i++){
	printf("%d ",arr2[i]);
}
return 0;
}
