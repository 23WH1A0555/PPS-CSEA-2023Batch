#include<stdio.h>
int main(){
	char num[10];
	int  temp,i,len,n;
	printf("enter any number:");
	for(i=0;i<10;i++){
	scanf("%c",&num[10]);
			}
for(i=0;num[i]!='\0';i++){
		len++;
			}
printf("length:%d",len);
printf("number:%s",num);
n=len;
 temp=num[0];
 num[0]=num[n];
 num[n]=num[0];
	for (i=0;i=n;i++){
	   printf("%c",num[10]);
                      }
       printf("number after swapping:%c",num[10]);
	return 0;
}
