// swapping of two numbers with temporary variable using functions
#include <stdio.h>
void swap(int *,int *);
int main()
{
    int a=10,b=12;
    printf ("numbers before swapping a=%d b=%d\n",a,b);
    swap(&a,&b);
    printf("numbers after swapping a=%d b=%d",a,b);
}
void swap(int *p1,int *p2)
{
    int temp;
    temp=*p1;
    *p1=*p2;
    *p2=temp;
}
       
