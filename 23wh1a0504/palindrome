#include<stdio.h>
int main(){
       int num,rev=0,rem;
  printf("enter any number");
  scanf("%d",&num);
    int temp=num;
  while(num>0){
   rem=num%10;
   rev=rev*10+rem;
   num=num/10;
               }
if(rev==temp)
      printf("given number is palindrome");
else
     printf("givrn number is not a palindrome");
    return 0;
     }
