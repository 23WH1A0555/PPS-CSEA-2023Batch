#include<stdio.h>
int main(){
          int num,i;
          printf("enter any number");
          scanf("%d",&num);
       int count=0;
    for(i=1;i<=num;i++){
      if(num%i==0)
      count++;}
        if(count==2)
   printf("given number is a prime number");
       else
   printf("given number is not a prime number");
    return 0;
    }
    
