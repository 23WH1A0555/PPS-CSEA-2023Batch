#include<stdio.h>
int linearsearch(int[],int,int);
int main(){
    int a[50],n,i,key;
    printf("enter the n");
    scanf("%d",&n);
    printf("enter the key");
    scanf("%d",&key);
    printf("enter the elements");
    for(i=0;i<n;i++)
    scanf("%d",&a[i]);
    int result=linearsearch(a,n,key);
    printf("%d",result);
}
int linearsearch(int a[],int n,int key){
    for(int i=0;i<n;i++){
         if(a[i]==key){
             return 1;
         }
        
    }
    return 0;
}
    
         
            
         
         
         
    
