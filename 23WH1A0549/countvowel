#include<stdio.h>
int vowelcount();
int main(){
    int result=vowelcount();
    printf("%d",result);
}
int vowelcount(){
    int count=0,i;
    char ch[20];
    printf("enter the string");
    scanf("%s",ch);
    for(i=0;i<20;i++){
        if(ch[i]=='a'||ch[i]=='e'||ch[i]=='i'||ch[i]=='o'||ch[i]=='u'||ch[i]=='A'||ch[i]=='E'||ch[i]=='I'||ch[i]=='O'||ch[i]=='U'){
            count++;
        }
    }
    return count;
}
         
            
         
         
         
    
