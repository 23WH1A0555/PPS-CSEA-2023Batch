#include<stdio.h>
int main()
{
 int arr1[20],arr2[20],size,i;
 printf("enter number of elements in an array: ");
scanf("%d",&size);
printf(" enter the elements in an array: ");
for(i=0; i<size;i++) {
    scanf("%d", &arr1[i]);
}
for (i=0; i<size;i++) {
    arr2[i]= arr1[i];
}
printf(" array 1 elements : ");
for(i=0;I<size;I++) {
    printf ("%d", arr1[I]);
}
printf ("\n");
printf(" array 2 elements: ");
for(I=0;I<size;I++) {
   printf("%d", arr2[1]);
}
}
