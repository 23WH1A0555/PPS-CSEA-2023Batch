#include <stdio.h>
int main()
{
    int num1,num2;
    printf("Enter first number:");
    scanf("%d",&num1);
    printf("Enter second number:");
    scanf("%d",&num2);
    if(num1>num2)
    {
     num1=num1+num2;
    num2=num1-num2;
    num1=(num1-num2);
    }
    else
    {
         num1=num1+num2;
        num2=(num1-num2);
        num1=(num1-num2);
    }
    printf("%d,%d",num1,num2);
}
